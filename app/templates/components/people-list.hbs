{{#each model as |person|}}
  {{#if person.isNew}}
    {{person-form model=person save-person=(route-action 'savePerson')}}
  {{else}}
    {{person-show model=person save-person=(route-action 'savePerson') delete-person=(route-action 'deletePerson') select-person=(action 'selectPerson') isSelectingAssociate=isSelectingAssociate select-associate=(action 'selectAssociate')}}
  {{/if}}
{{/each}}
