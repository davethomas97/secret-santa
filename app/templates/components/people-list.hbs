{{#each savedPeople as |person|}}
    {{person-show model=person save-person=(action 'savePerson') delete-person=(action 'deletePerson') select-person=(action 'selectPerson') isSelectingAssociate=isSelectingAssociate select-associate=(action 'selectAssociate')}}
{{/each}}

<button {{action "randomize" savedPeople}}>Randomize</button>
<button {{action "reset" savedPeople}}>Reset</button>
